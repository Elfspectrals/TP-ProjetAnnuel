{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend API avec PostgreSQL et Prisma",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "test -f dist/index.js && node dist/index.js || (npm run build && node dist/index.js)",
    "dev": "tsx watch src/index.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/seed.ts",
    "postinstall": "prisma generate",
    "prisma:generate-all": "PRISMA_CLI_BINARY_TARGETS=debian-openssl-3.0.x,debian-openssl-1.1.x,linux-musl,linux-musl-openssl-3.0.x prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "prisma": "^5.7.1",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2"
  },
  "keywords": [
    "api",
    "postgresql",
    "prisma",
    "express"
  ],
  "author": "",
  "license": "ISC"
}
